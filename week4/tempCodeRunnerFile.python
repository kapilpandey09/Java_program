# 2. Perform at least five basic arithmetic operations on NumPy arrays (addition of two arrays, subtraction, etc.).

import numpy as np

a = np.array([10, 20, 30])
b = np.array([1, 2, 3])

print("Addition:", a + b)
print("Subtraction:", a - b)
print("Multiplication:", a * b)
print("Division:", a / b)
print("Power:", a ** 2)